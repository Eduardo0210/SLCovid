    function filter() {

        var categoria = document.getElementById("SelCategoria").value;
        var sector = document.getElementById("SelSector").value;
        var anio = document.getElementById("SelAnio").value;
        var estado = document.getElementById("SelEstado").value;

        data = { categoria: categoria,sector : sector, anio : anio,estado : estado };

        $.ajax({
            url: "@Url.Action("SearchPractice", "Home")",
            data: data,
            type: "post",

        }).done(async function (data) {
            document.getElementById("listaModal").innerHTML = "";
            var json = JSON.parse(data);
            console.log(json);
            LimpiarMapa();
            for (var i = 0; i < json.length; i++) {

                try {
                    var estado = json[i].estado;
                    json[i].nomeclatura = await obtenerIDMapa(json[i].estado);
                    var idEstado = await obtenerIDMapa(json[i].estado);
                    document.getElementById(idEstado).style.fill = "var(--pink)";
                    document.getElementById(idEstado).style.cursor = "pointer";
                    //document.getElementById("listaModal").innerHTML = document.getElementById("listaModal").innerHTML + "<li><a href='Home/PracticeView?kiidPractica="+json[i].kiidPractica+"'>" + json[i].nombre + "</a></li>";
                    document.getElementById(idEstado).addEventListener("click", function () { mostrarModal(this) }, false);
                } catch (e) {

                }

            }
            jsonActual = json;
        })

    }

    function mostrarModal(elemento) {
        document.getElementById("listaModal").innerHTML = "";
        for (var i = 0; i < jsonActual.length; i++) {
            if (jsonActual[i].nomeclatura == elemento.id) {
                document.getElementById("listaModal").innerHTML = document.getElementById("listaModal").innerHTML + "<li><a href='Home/PracticeView?kiidPractica=" + jsonActual[i].kiidPractica + "'>" + jsonActual[i].nombre + "</a></li>";
            }
        }
        document.getElementById("tituloModal").innerText = "Buenas prácticas";
        $('#myModal').modal('show');
    }

