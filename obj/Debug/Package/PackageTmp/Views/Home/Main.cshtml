    async function obtenerIDMapa(estado) {
        var response = "";
        switch (estado) {
            case "Aguascalientes":
                response = "AGS";
                break;
            case "Baja California":
                response = "BCN";
                break;
            case "Baja California Sur":
                response = "BCS";
                break;
            case "Campeche":
                response = "CAM";
                break;
            case "Chiapas":
                response = "CHP";
                break;
            case "Chihuahua":
                response = "CHI";
                break;
            case "Ciudad de México":
                response = "CMX";
                break;
            case "Coahuila":
                response = "COA";
                break;
            case "Colima":
                response = "COL";
                break;
            case "Durango":
                response = "DUR";
                break;
